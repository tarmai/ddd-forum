datasource db {
  provider = "postgresql"
  url      = "postgres://postgres:posgress@localhost:5432/ddd-forum"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @default(uuid())
  email String  @unique
  username String @unique
  firstName String
  lastName String
  password String

  @@map("users")
}